#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int num, original, digit, sum;
    num=371;
    original=num;
    digit=num;
    sum=0;
    while(num!=0)
    {
        digit=num%10;
       sum=sum+(pow(digit, 3));
       num=num/10;
    }
    if(original==sum)
    printf("Armstrong!!");
    else printf("Not armstrong!");
}
