#include <iostream>
using namespace std;
int main()
{
    int i,n;
    int arr[100];
    cout<<"Enter the size of array"<<endl;
    cin>>n;
    cout<<"Enter any number"<<endl;
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<n;i++)
    {
        if(arr[i]+1!=arr[i+1])
        {cout<<arr[i]+1<<endl;
        break;}
    }
}
