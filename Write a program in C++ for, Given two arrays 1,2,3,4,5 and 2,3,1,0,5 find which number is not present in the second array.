#include <iostream>
#include<cmath>
using namespace std;

int main()
{
    int arr1[] = {1, 2, 3, 4, 5};
    int arr2[] = {2, 3, 1, 0, 5};
    int n1 = sizeof(arr1) / sizeof(arr1[0]);
    int n2 = sizeof(arr2) / sizeof(arr2[0]);
    int diff=0;
    int sum1=0;
    int sum2=0;
    for (int i = 0; i < n1; ++i) {
        sum1 += arr1[i];
    }
    for (int i = 0; i < n2; ++i) {
        sum2 += arr2[i];
    }
    diff=(sum1-sum2);
    cout << "Number not present in the second array: " << diff << endl;
    
}
